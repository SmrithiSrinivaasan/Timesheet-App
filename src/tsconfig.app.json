{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "esnext",
    "target": "es5",
    "types": [],
    "paths": {
      "@angular/*": ["../node_modules/@angular/*"]
    }
  },
  "emitDecoratorMetadata": true,
  "experimentalDecorators": true,
  "angularCompilerOptions": {
    "enableIvy": false
  },
  "files": ["main.ts", "polyfills.ts"],
  "include": ["**/*.d.ts"]
}
